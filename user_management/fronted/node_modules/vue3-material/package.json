{
    "name": "vue3-material",
    "version": "1.0.8",
    "description": "Vue 3 components which adhere to the material design spec",
    "license": "MIT",
    "repository": "deltabrot/vue3-material",
    "keywords": [
        "material",
        "material design",
        "vue",
        "vue3",
        "component",
        "components",
        "typescript"
    ],
    "main": "dist/vue3-material.ssr.js",
    "browser": "dist/vue3-material.esm.js",
    "module": "dist/vue3-material.esm.js",
    "unpkg": "dist/vue3-material.min.js",
    "types": "vue3-material.d.ts",
    "files": [
        "dist/*",
        "vue3-material.d.ts",
        "src/**/*.vue"
    ],
    "sideEffects": false,
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "serve": "vue-cli-service serve dev/serve.ts",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@rollup/plugin-alias": "^3.1.1",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@vue/cli-plugin-babel": "^4.5.10",
        "@vue/cli-plugin-typescript": "^4.5.10",
        "@vue/cli-service": "^4.5.10",
        "@vue/compiler-sfc": "^3.0.5",
        "cross-env": "^7.0.3",
        "minimist": "^1.2.5",
        "postcss": "^8.2.3",
        "rollup": "^2.36.1",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^6.0.0",
        "typescript": "^3.8.3",
        "vue": "^3.0.5"
    },
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "engines": {
        "node": ">=12"
    },
    "dependencies": {
        "vue-router": "^4.0.4"
    }
}
