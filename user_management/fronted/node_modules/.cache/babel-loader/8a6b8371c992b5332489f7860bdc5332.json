{"ast":null,"code":"export default {\n  data() {\n    return {\n      name: \"\",\n      age: 0\n    };\n  },\n\n  methods: {\n    getData(index) {\n      let url = \"http://localhost:8081/api/v1/user/\" + index;\n      this.axios.get(url, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(result => {\n        console.log(result);\n        this.name = result.data.name;\n        this.age = result.data.age;\n      });\n    },\n\n    updateData() {\n      let url = \"http://localhost:8081/api/v1/user/\" + this.$route.params.id;\n      this.axios.request({\n        url: url,\n        data: {\n          name: this.name,\n          age: this.age\n        },\n        method: 'put'\n      }).then(response => {\n        console.log(response.data.status);\n      }).catch(e => {\n        window.location.replace(\"http://localhost:8080/\");\n        alert(\"錯誤\");\n        console.log(e);\n        console.log(url);\n      });\n    }\n\n  },\n\n  created() {\n    this.getData(this.$route.params.id);\n  }\n\n};","map":{"version":3,"mappings":"AAeA,eAAe;EACbA,IAAI,GAAE;IACJ,OAAM;MACLC,IAAI,EAAC,EADA;MAELC,GAAG,EAAC;IAFC,CAAN;EAID,CANY;;EAObC,OAAO,EAAC;IACNC,OAAO,CAACC,KAAD,EAAQ;MACb,IAAIC,GAAE,GAAI,uCAAqCD,KAA/C;MACA,KAAKE,KAAL,CAAWC,GAAX,CAAeF,GAAf,EAAoB;QAClBG,OAAO,EAAE;UACP,gBAAgB;QADT;MADS,CAApB,EAIGC,IAJH,CAKGC,MAAD,IAAY;QACVC,OAAO,CAACC,GAAR,CAAYF,MAAZ;QACA,KAAKV,IAAL,GAAYU,MAAM,CAACX,IAAP,CAAYC,IAAxB;QACA,KAAKC,GAAL,GAAWS,MAAM,CAACX,IAAP,CAAYE,GAAvB;MAED,CAVH;IAWD,CAdK;;IAeNY,UAAU,GAAE;MACV,IAAIR,GAAE,GAAI,uCAAqC,KAAKS,MAAL,CAAYC,MAAZ,CAAmBC,EAAlE;MACA,KAAKV,KAAL,CAAWW,OAAX,CAAmB;QACjBZ,GAAG,EAACA,GADa;QAEhBN,IAAI,EAAC;UACFC,IAAI,EAAC,KAAKA,IADR;UAEFC,GAAG,EAAC,KAAKA;QAFP,CAFW;QAKbiB,MAAM,EAAC;MALM,CAAnB,EAMGT,IANH,CAOGU,QAAD,IAAc;QACbR,OAAO,CAACC,GAAR,CAAYO,QAAQ,CAACpB,IAAT,CAAcqB,MAA1B;MAEA,CAVH,EAUKC,KAVL,CAUWC,CAAC,IAAE;QAAEC,MAAM,CAACC,QAAP,CAAgBC,OAAhB,CAAwB,wBAAxB;QACZC,KAAK,CAAC,IAAD,CAAL;QACAf,OAAO,CAACC,GAAR,CAAYU,CAAZ;QA