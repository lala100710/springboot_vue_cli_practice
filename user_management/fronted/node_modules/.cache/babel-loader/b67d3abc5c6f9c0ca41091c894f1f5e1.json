{"ast":null,"code":"export default {\n  data() {\n    return {\n      name: \"\",\n      age: 0\n    };\n  },\n\n  methods: {\n    getData(index) {\n      let url = \"http://localhost:8081/api/v1/user/\" + index;\n      this.axios.get(url, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(result => {\n        console.log(result);\n        this.name = result.data.name;\n        this.age = result.data.age;\n      });\n    },\n\n    updateData() {\n      let url = \"http://localhost:8081/api/v1/user/\" + this.$route.params.id;\n      this.axios.request({\n        url: url,\n        data: {\n          name: this.name,\n          age: this.age\n        },\n        method: 'put'\n      }).then(response => {\n        window.location.replace(\"http://localhost:8080/\");\n        console.log(response.status);\n      }).catch(e => {\n        alert(\"錯誤\");\n        console.log(e);\n      });\n    }\n\n  },\n\n  created() {\n    this.getData(this.$route.params.id);\n  }\n\n};","map":{"version":3,"mappings":"AAwBA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,GAAG,EAAE;IAFA,CAAP;EAID,CANY;;EAObC,OAAO,EAAE;IACPC,OAAO,CAACC,KAAD,EAAQ;MACb,IAAIC,GAAE,GAAI,uCAAuCD,KAAjD;MACA,KAAKE,KAAL,CAAWC,GAAX,CAAeF,GAAf,EAAoB;QAClBG,OAAO,EAAE;UACP,gBAAgB;QADT;MADS,CAApB,EAIGC,IAJH,CAKGC,MAAD,IAAY;QACVC,OAAO,CAACC,GAAR,CAAYF,MAAZ;QACA,KAAKV,IAAL,GAAYU,MAAM,CAACX,IAAP,CAAYC,IAAxB;QACA,KAAKC,GAAL,GAAWS,MAAM,CAACX,IAAP,CAAYE,GAAvB;MAED,CAVH;IAWD,CAdM;;IAePY,UAAU,GAAG;MACX,IAAIR,GAAE,GAAI,uCAAuC,KAAKS,MAAL,CAAYC,MAAZ,CAAmBC,EAApE;MACA,KAAKV,KAAL,CAAWW,OAAX,CAAmB;QACjBZ,GAAG,EAAEA,GADY;QAEjBN,IAAI,EAAE;UACJC,IAAI,EAAE,KAAKA,IADP;UAEJC,GAAG,EAAE,KAAKA;QAFN,CAFW;QAKdiB,MAAM,EAAE;MALM,CAAnB,EAMGT,IANH,CAOGU,QAAD,IAAc;QAEZC,MAAM,CAACC,QAAP,CAAgBC,OAAhB,CAAwB,wBAAxB;QACAX,OAAO,CAACC,GAAR,CAAYO,QAAQ,CAACI,MAArB;MAED,CAZH,EAYKC,KAZL,CAYWC,KAAK;QACZC,KAAK,CAAC,IAAD,CAAL;QACAf,OAAO,CAACC,GAAR,CAAYa,CAAZ;MAED,CAhBH;IAkBF;;EAnCO,CAPI;;EA4CbE,OAAO,GAAG;IACR